function determine_region(x, y, is_make){
  if(determine_arc_region(x, y, innerWidth / 7.1, innerWidth / 7.1, innerWidth /2 + x_offset, innerWidth / 17)){
    insert_shot_in_array(13, is_make, 1);
  }
