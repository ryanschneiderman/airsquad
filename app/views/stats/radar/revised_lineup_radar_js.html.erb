	function calcAggregateTSPct(shooting_stats){
		var len = shooting_stats.length
		var points;
		var ft_makes;
		var ft_misses;
		var fg_makes;
		var fg_misses
		for(var i = 0; i < len; i++){
			if(shooting_stats[i].id == "13"){
